#!/bin/bash
# This file helps diagnose Vite issues
# Run this if you get "Proxy error: Connection refused"

echo "Vite troubleshooting steps:"
echo "1. Kill any stuck processes on port 5173"
echo "2. Clear Vite cache"
echo "3. Restart dev server"
echo ""
echo "Commands to run:"
echo "  fuser -k 5173/tcp 2>/dev/null || true"
echo "  rm -rf node_modules/.vite"
echo "  npm run dev"
